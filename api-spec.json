{
  "openapi": "3.0.0",
  "info": {
    "title": "Project Cactus API",
    "description": "This is the API of Project Cactus",
    "license": {
      "name": "Apache 2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "/api"
    }
  ],
  "paths": {
    "/libraries": {
      "get": {
        "tags": ["general"],
        "summary": "get all libraries",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "tags": ["general"],
        "summary": "adds new library",
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Access token is missing or invalid"
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
    "/libraries/{id}/": {
      "get": {
        "tags": ["general"],
        "summary": "get specified library",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the library to get",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      },
      "post": {
        "tags": ["general"],
        "summary": "adds version",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "description": "ID of the library to get",
            "required": true,
            "style": "simple",
            "explode": false,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Access token is missing or invalid"
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    },
    "/auth/login": {
      "post": {
        "tags": ["general"],
        "summary": "logs the user in",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/auth/register": {
      "post": {
        "tags": ["general"],
        "summary": "registers new user",
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/testlab": {
      "post": {
        "tags": ["general"],
        "summary": "post testresults from testlab",
        "responses": {
          "200": {
            "description": "OK"
          },
          "401": {
            "description": "Access token is missing or invalid"
          }
        },
        "security": [
          {
            "bearerAuth": []
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {},
    "responses": {
      "UnauthorizedError": {
        "description": "Access token is missing or invalid"
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}
